{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
    "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": false,
      "prConcurrentLimit": 0,
      "prHourlyLimit": 0,
      "enabled": false
    }
  ],
   "rollbackPrs": true,
   "dependencyDashboard": true,
   "dependencyDashboardAutoclose": true,
}
